<div id="<%= dom_id order %>">
  <p>
    <strong>Order code:</strong>
    <%= order.order_code %>
  </p>

  <p>
    <strong>Bill no:</strong>
    <%= order.bill_no %>
  </p>

  <p>
    <strong>Product name:</strong>
    <%= order.product_name %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= order.quantity %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= order.price %>
  </p>

  <p>
    <strong>Discount:</strong>
    <%= order.discount %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= order.date %>
  </p>

  <p>
    <strong>Customer code:</strong>
    <%= order.customer_code %>
  </p>

  <p>
    <strong>Customer:</strong>
    <%= order.customer_id %>
  </p>

</div>
